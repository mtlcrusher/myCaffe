		decay_mult	: 0
	}

	# beta
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	scale_param {
		filler {
			value	: 1
		}
		bias_term	: true
		bias_filler {
			value	: 0
		}
	}
}

layer {
	name	: "relu3"
	type	: "ReLU"
	bottom	: "conv3/pw"
	top		: "conv3/pw"

	relu_param {
		negative_slope	: 0.01
	}
}

# end of mobilenet layer

# start of mobile net layer


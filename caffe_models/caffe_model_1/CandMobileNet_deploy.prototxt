		decay_mult	: 0
	}

	# name s
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	# name b
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	batch_norm_param {
		use_global_stats	: true
	}

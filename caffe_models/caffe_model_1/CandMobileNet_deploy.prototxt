name: "CandMobileNet"

layer {
	name: "data_in"
	type: "Input"
	top: "data"

	input_param {
		shape: {
			dim: 1
			dim: 3
			dim: 192
			dim: 192
		}
	}
}

layer {
	name	: "conv1"
	type	: "Convolution"
	bottom	: "data"
	top		: "conv1"

	# filter
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	# bias
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	convolution_param {
		num_output	: 32
		kernel_size	: 3
		stride		: 2
		pad			: 1

		weight_filler {
	      type: "msra"
	    }
	}
}

layer {
	name	: "conv1/bn"
	type	: "BatchNorm"
	bottom	: "conv1"
	top		: "conv1"

	# name m
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	# name s
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	# name b
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	batch_norm_param {
		use_global_stats	: true
	}
}

layer {
	name	: "conv1/scale"
	type	: "Scale"
	bottom	: "conv1"
	top		: "conv1"

	# alpha
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	# beta
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	scale_param {
		filler {
			value	: 1
		}
		bias_term	: true
		bias_filler {
			value	: 0
		}
	}
}

layer {
	name	: "relu1"
	type	: "ReLU"
	bottom	: "conv1"
	top		: "conv1"

	relu_param {
		negative_slope	: 0.01
	}
}

# start of mobile net layer

layer {
	name	: "conv2/dw"
	type	: "Convolution"
	bottom	: "conv1"
	top		: "conv2/dw"

	# filter
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	# bias
	param {
		lr_mult		: 1
		decay_mult	: 0
	}
	
	convolution_param {
		num_output	: 32
	    bias_term	: false
	    pad			: 1
	    kernel_size	: 3
	    group		: 32
	    engine		: CUDNN
	    stride		: 1
	   
	    weight_filler {
	      type: "msra"
	    }
	}
}

layer {
	name	: "conv2/bn"
	type	: "BatchNorm"
	bottom	: "conv2/dw"
	top		: "conv2/dw"

	# name m
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	# name s
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	# name b
	param {
		lr_mult		: 0
		decay_mult	: 0
	}

	batch_norm_param {
		use_global_stats	: true
	}
}

layer {
	name	: "conv2/scale"
	type	: "Scale"
	bottom	: "conv2/dw"
	top		: "conv2/dw"

	# alpha
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	# beta
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	scale_param {
		filler {
			value	: 1
		}
		bias_term	: true
		bias_filler {
			value	: 0
		}
	}
}

layer {
	name	: "relu2"
	type	: "ReLU"
	bottom	: "conv2/dw"
	top		: "conv2/dw"

	relu_param {
		negative_slope	: 0.01
	}
}

layer {
	name	: "conv3/pw"
	type	: "Convolution"
	bottom	: "conv2/dw"
	top		: "conv3/pw"

	# filter
	param {
		lr_mult		: 1
		decay_mult	: 0
	}

	# bias
	param {
		lr_mult		: 1
		decay_mult	: 0
	}
	
	convolution_param {
		num_output	: 64
	    bias_term	: false
	    pad			: 0
	    kernel_size	: 1
	    engine		: CUDNN
	    stride		: 1
	   
	    weight_filler {
	      type: "msra"
	    }
	}
}

layer {
	name	: "conv3/bn"
	type	: "BatchNorm"
	bottom	: "conv3/pw"
	top		: "conv3/pw"

	# name m
	param {

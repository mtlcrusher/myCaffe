layer {
	name	: "pool1"
	type	: "Pooling"
	bottom	: "conv7/pw"
	top		: "pool1"
	pooling_param {
		pool			: AVE
		global_pooling	: true
	}
}

layer {
	name	: "fc7"
	type	: "Convolution"
	bottom	: "pool1"
	top		: "fc7"
	param {
		lr_mult		: 1
		decay_mult	: 1
	}
	param {
		lr_mult		: 2
		decay_mult	: 0
	}
	convolution_param {
		num_output	: 2
		kernel_size	: 1
		weight_filler {
			type	: "msra"
		}
		bias_filler {
			type	: "constant"
			value	: 0
		}
	}
}
layer {
	name	: "prob"
	type	: "Softmax"
	bottom	: "fc7"
	top		: "prob"
}